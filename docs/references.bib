@article{Erosa1994,
abstract = {In designing optimizing and parallelizing compilers, it is often$\backslash$nsimpler and more efficient to deal with programs that have structured$\backslash$ncontrol flow. Although most programmers naturally program in a$\backslash$nstructured fashion, there remain many important programs and benchmarks$\backslash$nthat include some number of goto statements, thus rendering the entire$\backslash$nprogram unstructured. Such unstructured programs cannot be handled with$\backslash$ncompilers built with analyses and transformations for structured$\backslash$nprograms. In this paper we present a straight-forward algorithm to$\backslash$nstructure C programs by eliminating all goto statements. The method$\backslash$nworks directly on a high-level abstract syntax tree (AST) representation$\backslash$nof the program and could easily be integrated into any compiler that$\backslash$nuses an AST-based intermediate representation. The actual algorithm$\backslash$nproceeds by eliminating each goto by first applying a sequence of$\backslash$ngoto-movement transformations followed by the appropriate$\backslash$ngoto-elimination transformation. We have implemented the method in the$\backslash$nMcCAT (McGill Compiler Architecture Testbed) optimizing/parallelizing C$\backslash$ncompiler and we present experimental results that demonstrate that the$\backslash$nmethod is both efficient and effective},
author = {a.M. Erosa and Hendren, L.J.},
