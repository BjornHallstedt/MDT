arxivId = {NIHMS150003},
author = {Fieremans, Els and Jensen, Jens H. and Helpern, Joseph A.},
doi = {10.1016/j.neuroimage.2011.06.006},
eprint = {NIHMS150003},
file = {:home/robbert/Documents/phd/papers/fieremans2011.pdf:pdf},
isbn = {1095-9572 (Electronic)$\backslash$r1053-8119 (Linking)},
issn = {10538119},
journal = {NeuroImage},
keywords = {Axonal density,Diffusion,Kurtosis,MRI,Microstructure,White matter},
number = {1},
pages = {177--188},
pmid = {21699989},
publisher = {Elsevier Inc.},
title = {{White matter characterization with diffusional kurtosis imaging}},
