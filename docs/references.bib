issn = {15507998},
journal = {Physical Review D - Particles, Fields, Gravitation and Cosmology},
number = {4},
pages = {1--20},
primaryClass = {gr-qc},
title = {{Use and abuse of the Fisher information matrix in the assessment of gravitational-wave parameter-estimation prospects}},
volume = {77},
year = {2008}
}
